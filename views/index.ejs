        //       placeholder: 'Select an option'
        //     });
        masterMode();
        collectionButton.on('click', function() {
            collectionMode();
        });
        masterButton.on('click', function() {
            masterMode();
        })
        cancelButton.on('click', function(e) {
            $('.addd').show();
            e.preventDefault();
            normalMode();
        });
        updateButton.on('click', function(e) {
            e.preventDefault();
            updatePixel();
        });

        messageButton.on('click', function(e) {
            e.preventDefault();
            sendMessage();
        });

        $('.btn').click(function(e) {
            if ($(this).hasClass('edit-btn') || $(this).attr('id') == 'cancelButton' || $(this).hasClass('clip') || $(this).hasClass('upgrade')) return;
            toastr.info('Please wait!');
        });

        $('#add').click(function(e) {
            e.preventDefault();
            addPixel();
        });

        $('#setting').change(function() {
            toastr.info('', 'Please Wait!');
            updateSetting();
        });

        $('body').on('click', '.del-btn', function() {
            let id = $(this).attr('pixelId');
            deletePixel(id);
            normalMode();
            resetInput();
        });

        $('body').on('click', '.edit-btn', function() {

            editMode();
            let id = $(this).attr('pixelId');
            editPixel(id);
        });
        $('body').on('click', '.saveother', function() {
            updateother();
        });


        //////////////END LISTENERS/////////////////

        ///////////////FUNCTIONS/////////////////

        function addPixel() {
            var selText=[];
            $('#collectionSelector option:selected').each(function () {
                var $this = $(this);
                if ($this.length) {
                    selText.push($this.text())
                }
            });
            let code = $('#code').val();
            let collections = $('#collectionSelector').val();
            let type = $("input[name='type']:checked").val();
            let id = Math.floor(Math.random() * 1000);
            collections = JSON.stringify(collections)
            $.ajax({
                url: "/addpixel",
                method: "POST",
                dataType: "json",
                data: {
                    type: type,
                    code: code,
                    collections: collections,
                    id : id,
                    colname :JSON.stringify(selText)
                },
                success: function(response) {
                    resetInput();
                    toastr.remove();
                    if (response.error) {
                        toastr.error(response.error_data);
                    } else {
                        toastr.success('Pixel added successfully');
                        addPixelRow(response.pixel);
                    }

                }
            });
        }

        function updatePixel() {
            var selText=[];
            $('#collectionSelector option:selected').each(function () {
                var $this = $(this);
                if ($this.length) {
                    selText.push($this.text())
                }
            })
            let code = $('#code').val();
            let collections = $('#collectionSelector').val();
            let type = $("input[name='type']:checked").val();
            let id = $('#idInput').val();
            $('#pixelRow' + id).remove();
            collections = JSON.stringify(collections)
            $.ajax({
                url: "/update_pixel",
                method: "POST",
                dataType: "json",
                data: {
                    shop: '',
                    id: id,
                    code: code,
                    collections: collections,
                    type: type,
                    colname :JSON.stringify(selText)

                },
                success: function(response) {
                    resetInput();
                    toastr.remove();
                    if (response.error) {
                        toastr.error(response.error_data);
                    } else {
                        toastr.success('Pixel updated successfully');
                        addPixelRow(response.pixel);
                        normalMode();
                    }

                }
            });
        }

        function deletePixel(id) {
            $.ajax({
                url: "/delete",
                method: "POST",
                data: {
                    shop: '',
                    id: id
                },
                success: function(response) {
                    toastr.remove();
                    if (response.error) {
                        toastr.error(response.error_data);
                    } else {
                        $('#pixelRow' + id).remove();
                        toastr.success(response.message);
                    }
                }
            });
        }



        function updateSetting() {
            let value = $('#setting').val();

            $.ajax({
                url: "/update",
                method: "POST",
                data: {
                    shop:'',
                    setting: value,
                },
                success: function(response) {
                    toastr.remove();
                    if (response.error) {
                        toastr.error(response.error_data);
                    } else {
                        toastr.success(response.message);
                    }

                }
            });
        }

        function updateother() {
            $.ajax({
                url: "/otheradd",
                method: "POST",
                data: $('.otherform').serialize(),
                success: function(response) {
                    toastr.remove();
                    if (response.error) {
                        toastr.error(response.error_data);
                    } else {
                        toastr.success(response.message);
                    }

                }
            });
        }




        function editPixel(id) {
            let type = $('#type' + id).text();
            let collections;
            $('#code').val($('#code' + id).text());
            idInput.val(id);
          if (type == 'collection') {
                collectionMode();
                collections = $('#collections' + id).text().split(' ').join('').split(',');
                $("#collectionSelector option").each(function(indexx, item) {
                    var vall = $(this).val();
                    collections.forEach(function(item2, index) {
                        var coll = item2;
                        if (parseInt(coll) == parseInt(vall)) {
                            $(item).prop('selected', true);
                        }
                    });
                });
                $('#collectionSelector').trigger('change');
            } else {
                masterMode();
            }
        }

        function resetInput() {
            $('#collectionRow').hide();
            $('#code').val('');
            $('#collectionSelector').val('');
            $("#masterPixel").prop("checked", true);
        }

        function resetMessageInput() {
            $('#email').val('');
            $('#subject').val('');
            $('#message').val('');
        }

        function masterMode() {
            masterButton.prop("checked", true);
            collectionRow.hide();
        }



        function collectionMode() {
            collectionButton.prop("checked", true);
            collectionRow.show();
        }

        function editMode() {
            normalButtons.hide();
            editButtons.show();
        }

        function normalMode() {
            masterMode();
            editButtons.hide();
            normalButtons.show();
            $('#code').val('');
            $('#collectionSelector').val('');
        }


        ///////////////END FUNCTIONS//////////////
    });
</script>
<script>
    var st = new XMLHttpRequest();
    var shop =getCookie('shop')
    var url = "http://multi-pixels.com/collection?store=" +shop;
    st.open("GET", url, true);
    st.onreadystatechange = function () {
        if (st.readyState === 4 && st.status === 200) {
            var _response = this.responseText
            var collections = JSON.parse(_response)
            collections =collections.custom_collections
            for (let i =0 ;i<collections.length;i++)
            {
                $('#collectionSelector').append($('<option>', {
                    value: collections[i].id,
                    text: collections[i].title
                }));
            }
        }
    };


    function getCookie(name) {
        const value = `; ${document.cookie}`;
        const parts = value.split(`; ${name}=`);
        if (parts.length === 2) return parts.pop().split(';').shift();
    }
    st.send();

</script>
