            } else {
                $('#ascript').hide(1000);
            }
        });
        $('#changeStatus').change(function() {
            let statu = $('#changeStatus').is(":checked")
            $.ajax({
                url: "/Change-Status",
                method: "POST",
                dataType: "json",
                data: {
                    statu: statu
                },
                success: function(response) {
                    toastr.remove();
                    if (response.status != true) {
                        toastr.error(response.error_data);
                    } else {
                        toastr.success('Status changed successfully');
                    }

                }
            });
        });
        // $('#list').paginate({perPage:10});

        /////////////VARIABLES////////////////////
        let selector = $('#collectionSelector');
        let idInput = $('#idInput');
        let collectionRow = $('#collectionRow');
        let masterButton = $('.masterPixel');
        let collectionButton = $('.collectionPixel');
        let settingSelector = $('#setting');
        let normalButtons = $('#normalButtons');
        let editButtons = $('#editButtons');
        let updateButton = $('#updateButton');
        let cancelButton = $('#cancelButton');
        let preferenceButton = $('#preferenceButton');
        let messageButton = $('#messageButton');
