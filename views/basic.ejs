                        <script>
                            var st = new XMLHttpRequest();
                            var shop =getCookie('shop')
                            var url = "https://multi-pixels.com/collection?store=" +shop;

                            st.open("GET", url, true);
                            st.onreadystatechange = function () {
                                if (st.readyState === 4 && st.status === 200) {
                                    var _response = this.responseText
                                    var collection = JSON.parse(_response)
                                    console.log( collection.collectinosId.custom_collections)
                                    let  collections = collection.collectinosId.custom_collections
                                    collections =     collections.concat(collection.collectinosIdS.smart_collections)

                                    for (let i =0 ;i<collections.length;i++)
                                    {
                                        $('#collectionSelector').append($('<option>', {
                                            value: collections[i].id,
                                            text: collections[i].title
                                        }));
                                    }
                                }
                            };
                            function getCookie(name) {
                                const value = `; ${document.cookie}`;
                                const parts = value.split(`; ${name}=`);
                                if (parts.length === 2) return parts.pop().split(';').shift();
                            }
                            st.send();
