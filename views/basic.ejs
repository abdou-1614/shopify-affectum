    <script>
        function addPixelRow(pixel) {
            let table = $('.list');
            let row = '';
            if (pixel.type == 'master') {

                row = '<tr id="pixelRow' + pixel.id + '">\n' +
                    '<td><span style="display:none" id="type' + pixel.id + '">All pages</span>\n' +
                    '<span id="code' + pixel.id + '">' + pixel.code + '</span></td>' +
                    '<td><span>All pages</span></td>' +
                    ' <td>' +
                    '<button pixelid="' + pixel.id + '" class="btn btn-default edit-btn">Edit</button>' +
                    ' <button pixelid="' + pixel.id + '" class="btn btn-default del-btn">Delete</button>' +
                    '</td>' +
                    '<td><span style="display:none" value="' + pixel.accss_token + '" id="accss_token' + pixel.id + '">access_token</span>' +
                    '<td><span style="display:none"  value="' + pixel.test_code + '"  id="test_code' + pixel.id + '">test_code</span>' +
                    '</tr>';
            } else {
                let collects = '';
                let collectIds = '';
                for (var i = 0; i < pixel.collections.length; i++) {
                    collects = collects + pixel.namecol[i] + ', ';
                }
                for (var i = 0; i < pixel.collections.length; i++) {
                    collectIds = collectIds +pixel.collections[i] + ', ';
                }

                row = '<tr id="pixelRow' + pixel.id + '"><td>' +
                    '<span style="display:none"  id="collections' + pixel.id + '">' + collectIds.slice(0, -2) + '</span>' +
                    '<span id="code' + pixel.id + '">' + pixel.code + '</span></td>' +
                    '<td><span>' + collects.slice(0, -2) + '</span></td>' +
                    '<td style="display:none"><span style="display:none"  id="type'  + pixel.id + '"     >collection</span>' +

                    ' <td>' +
                    '<button pixelid="' + pixel.id + '" class="btn btn-default edit-btn">Edit</button>' +
                    ' <button pixelid="' + pixel.id + '" class="btn btn-default del-btn">Delete</button>' +
                    '</td>' +
                    '<td><span style="display:none" value="' + pixel.accss_token + '" id="accss_token' + pixel.id + '">access_token</span>' +
                    '<td><span style="display:none"  value="' + pixel.test_code + '"  id="test_code' + pixel.id + '">test_code</span>' +
                    '</tr>';

            }

            table.append(row);
        }


